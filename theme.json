							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/open-sans_italic_500.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/open-sans_italic_700.ttf"
							]
						}
					],
					"fontFamily": "Open Sans",
					"slug": "open-sans"
				}
			],
			"fontSizes": [
				{
					"name": "Hero",
					"size": "4.0689rem",
					"slug": "hero"
				},
				{
