				{
					"name": "XX Small",
					"size": "0.6827rem",
					"slug": "xx-small"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"border": {},
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "600",
					"letterSpacing": "1px"
				},
				"variations": {
					"outline": {
						"border": {
							"bottom": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "3px"
							},
							"left": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "3px"
							},
							"right": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "3px"
							},
							"top": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "3px"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			},
			"core/group": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--medium)",
						"top": "var(--wp--preset--spacing--medium)"
					},
					"padding": {
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--medium)",
						"top": "var(--wp--preset--spacing--medium)"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.2"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-fonts)"
				}
			}
		},
		"filter": {},
		"spacing": {},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-fonts)",
			"fontSize": "1.15rem",
			"lineHeight": "1.5"
		}
	},
	"version": 3
}