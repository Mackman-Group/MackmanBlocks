					"size": "3.2551rem",
					"slug": "xx-large"
				},
				{
					"name": "Extra Large",
					"size": "2.604rem",
					"slug": "extra-large"
				},
				{
					"name": "Large",
					"size": "2.0836rem",
					"slug": "large"
				},
				{
					"name": "Medium",
					"size": "1.6667rem",
					"slug": "medium"
				},
				{
