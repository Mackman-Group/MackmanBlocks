{
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#f4f4f4",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#1a382e",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#bca35d",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#ffffff",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#e4eeed",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			]
		},
		"layout": {
			"contentSize": "1140px",
			"wideSize": "1440px"
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingScale": {
				"increment": 1.8,
				"mediumStep": 1.8,
				"operator": "*",
				"steps": 5,
				"unit": "rem"
			}
		},
		"typography": {
			"customFontSize": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Fonts",
					"slug": "system-fonts"
				},
				{
					"fontFamily": "Geneva, Tahoma, Verdana, sans-serif",
					"slug": "geneva-verdana"
				},
				{
					"fontFamily": "Cambria, Georgia, serif",
					"slug": "cambria-georgia"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Open Sans",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/open-sans_normal_300.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/open-sans_normal_500.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/open-sans_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/open-sans_italic_300.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/open-sans_italic_500.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/open-sans_italic_700.ttf"
							]
						}
					],
					"fontFamily": "Open Sans",
					"slug": "open-sans"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "14px",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "16px",
					"slug": "normal"
				},
				{
					"name": "Medium",
					"size": "32px",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "48px",
					"slug": "large"
				},
				{
					"name": "Hero",
					"size": "78px",
					"slug": "hero"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "600",
					"letterSpacing": "1px"
				},
				"variations": {
					"outline": {
						"border": {
							"bottom": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "3px"
							},
							"left": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "3px"
							},
							"right": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "3px"
							},
							"top": {
								"color": "var(--wp--preset--color--contrast)",
								"style": "solid",
								"width": "3px"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-fonts)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-fonts)",
			"fontSize": "1.15rem",
			"lineHeight": "1.5"
		}
	},
	"version": 2
}